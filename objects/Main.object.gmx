            <string>// init
draw_set_font(Font);

// generate sprites
Tileset = sprite_add("tileset.png", 1, false, false, 0, 0)

var surface = surface_create(sprite_get_width(Tileset), sprite_get_height(Tileset));

surface_set_target(surface);
draw_sprite(Tileset, 0, 0, 0);
surface_reset_target();

// tiles
Tiles = ds_list_create();

// new tile
var Tile;

Tile = ds_list_create();
ds_list_add(Tile, 0); // id
ds_list_add(Tile, sprite_create_from_surface(surface, 0, 0, 16, 16, false, false, 0, 0)); // sprite
ds_list_add(Tile, "floor"); // inspection info
ds_list_add(Tiles, Tile); // add it to the global tile set

Tile = ds_list_create();
ds_list_add(Tile, 1);
ds_list_add(Tile, sprite_create_from_surface(surface, 16, 0, 16, 16, true, false, 0, 0));
ds_list_add(Tile, "player");
ds_list_add(Tiles, Tile);

surface_free(surface);

// temp player
player_x = room_width/2;
player_y = room_height/2;
</string>
